{
  "name": "Kiara Music",
  "description": "Open-Source bot to play songs in your Telegram's Group Voice Chat. Powered by MUSIK KU.",
  "logo": "https://telegra.ph/file/753de9d023e666dda1b36.jpg",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/HarshPanditXD/Kiara-Music",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "String Session Pyrogram",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Bot Token @BotFather",
      "required": true
    },
    "BOT_USERNAME": {
      "description": "Username bot @",
      "required": true
    },
    "BOT_NAME": {
      "description": "Nama Bot MusicPlayer",
      "required": false,
      "value": ""
   },
    "SUPPORT_GROUP": {
      "description": "support Grup MusicPlayer",
      "required": false,
      "value": "Kiara_Support"
    },
    "UPDATES_CHANNEL": {
      "description": "Updates Channel @ [FILL THE CHANNEL]",
      "required": false,
      "value": "THE_SECRET_WORLDS"
    }, 
    "PROJECT_NAME": {
      "description": "Name Kiara MusicPlayer.",
      "required": false,
      "value": "KIARA PROJECT"
    },
    "ASSISTANT_NAME": {
      "description": "Username Telegram asisten MusicPlayer Anda tanpa @.",
      "required": true
    },
    "OWNER": {
      "description": "NamE Username Telegram",
      "required": true
    },
    "ARQ_API_KEY": {
      "description": "arq api fill bot telegram Â» t.me/ARQRobot atau @ARQRobot",
      "required": true
    },    
    "BG_IMAGE": {
      "description": "Custom Background Play Musik Gunakan Foto yang Transparant format .png / .jpg",
      "required": false,
      "value": "https://telegra.ph/file/c256c969058325e497578.jpg"
    },        
    "API_ID": {
      "description": "API ID  my.telegram.org/apps @Kenkanrobot",
      "required": true
    },   
    "API_HASH": {
      "description": "API HASH my.telegram.org/apps @Kenkanrobot",
      "required": true
    },
    "PMPERMIT": {
      "description": "PMPERMIT",
      "required": false,
      "value": "ENABLE"
    }, 
    "SUDO_USERS": {
      "description": "User ID Telegram",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "audio download (minutes).",
      "required": true,
      "value": "300"
    }
  }
}
